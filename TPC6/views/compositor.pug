extends layout

block content
  .w3-card-4
    header.w3-container.w3-pink 
      h1= title
    .w3-container
        ul.w3-ul.w3-card-4
            li Id: #{compositor.id}
            li bio: #{compositor.bio}
            li Data de Nascimento: #{compositor.dataNasc}
            li Data de Óbito: #{compositor.dataObito}
            li 
                a(href="periodos/" + compositor.periodo) Período: #{compositor.periodo}

    footer.w3-container.w3-teal
        address Gerado por gcompositor::RPCW2022 em #{d} - 
            a(href='/') Voltar
extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1= title
    .w3-container
      table.w3-table-all
        tr
          th Id
          th Nome
          th Periodo
        each compositor in compositores
          tr
            td= compositor.id
            td
              a(href=`/compositores/${compositor.id}`)= compositor.nome
            td= compositor.periodo
    footer.w3-container.w3-teal
      address Gerado por galuno::RPCW2022 em #{d} - #[a(href="/") Voltar]